{
  "buildCommand": "npm run build --prefix client",
  "outputDirectory": "client/dist",
  "installCommand": "npm install --prefix client && npm install --prefix backend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/api"
    }
  ]
}
