{
  "name": "portfolio-project",
  "version": "1.0.0",
  "description": "Full-stack portfolio project with admin panel",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm start",
    "client": "cd client && npm run dev",
    "seed": "cd backend && npm run seed",
    "start": "npm run dev"
  },
  "keywords": ["portfolio", "react", "node", "mongodb"],
  "author": "Timucin",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "@vercel/blob": "^0.23.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
